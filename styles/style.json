      "paint": {
        "text-color": "#495e91",
        "text-halo-color": "rgba(255,255,255,0.7)",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "water_name_line_label",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "water_name",
      "filter": [
        "match",
        ["geometry-type"],
        ["LineString", "MultiLineString"],
        true,
        false
      ],
      "layout": {
        "symbol-placement": "line",
        "symbol-spacing": 350,
        "text-field": [
          "case",
          ["has", "name:nonlatin"],
          ["concat", ["get", "name:latin"], " ", ["get", "name:nonlatin"]],
          ["coalesce", ["get", "name_en"], ["get", "name"]]
